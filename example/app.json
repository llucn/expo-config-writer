{
  "expo": {
    "name": "expo-config-writer-example",
    "slug": "expo-config-writer-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.configwriter.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "expo.modules.configwriter.example",
      "allowBackup": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [[
      "../app.plugin.js", {
        "ios": {
          "podfile": {
            "tag": "react-native-photo-edit",
            "anchor": "prepare_react_native_project!",
            "offset": -1,
            "newSrc": [
              "pod 'SDWebImage', '~> 5.11.1', :modular_headers => true", 
              "pod 'SDWebImageWebPCoder', '~> 0.8.4', :modular_headers => true"
            ],
            "comment": "#"
          }
        }
      }
    ]]
  }
}
